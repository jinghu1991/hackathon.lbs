#============================================================================
# Mysql config
#============================================================================
db.driver=com.mysql.jdbc.Driver

db.online.master.url=${jdbc_master_url}
db.online.master.username=${jdbc_master_username}
db.online.master.password=${jdbc_master_password}

db.online.slave.url=${jdbc_slave_url}
db.online.slave.username=${jdbc_slave_username}
db.online.slave.password=${jdbc_slave_password}

pool.initialPoolSize=0
pool.minPoolSize=0
pool.maxPoolSize=100
pool.maxIdleTime=120
pool.acquireIncrement=1
